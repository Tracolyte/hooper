{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(favicon\\.ico|vite\\.svg|.*\\.(png|jpg|jpeg|gif|svg|ico))",
      "dest": "/dist/public/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/dist/public/index.html"
    }
  ]
}